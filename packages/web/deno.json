{
  "tasks": {
    "dev": "deno run -A npm:vite & deno run server:start",
    "build": "deno run -A npm:vite build",
    "server:start": "deno run -A --watch ./api/main.ts",
    "serve": "deno run build && deno run server:start"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@heroicons/react": "npm:@heroicons/react@^2.2.0",
    "@oak/oak": "jsr:@oak/oak@^17.1.4",
    "@std/assert": "jsr:@std/assert@^1.0.12",
    "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1",
    "@types/react": "npm:@types/react@^19.1.2",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.4.1",
    "gray-matter": "npm:gray-matter@^4.0.3",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "react-markdown": "npm:react-markdown@^8.0.5",
    "react-router-dom": "npm:react-router-dom@^7.5.1",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.17",
    "tailwindcss": "npm:tailwindcss@^4.1.17",
    "vite": "npm:vite@5.4.21",
        "tailwind-scrollbar": "npm:tailwind-scrollbar@^4.0.2"
  },
    "nodeModulesDir": "auto",
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}