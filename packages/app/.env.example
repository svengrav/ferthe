# Ferthe App Environment Variables
# Copy to .env and adjust values

# Environment
EXPO_PUBLIC_ENVIRONMENT=development

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:7000/core/api/v1
EXPO_PUBLIC_API_TIMEOUT=5000

# Storage
EXPO_PUBLIC_JSON_STORE_URL=http://localhost:7003

# Debug Features
EXPO_PUBLIC_ENABLE_LOGGER=true
EXPO_PUBLIC_MAP_DEBUG=true
